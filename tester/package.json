{
  "name": "react-native-harmony-tester",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "preinstall": "cd ../react-native-harmony && npm pack && cd ../tester",
    "postinstall": "node ./scripts/create-build-profile && npm uninstall react-native-harmony && npm i react-native-harmony@file:../react-native-harmony/rnoh-react-native-harmony-0.72.11.tgz",
    "dev:cli": "cd ../react-native-harmony && npm run install:dev-cli && cd ../tester && npm i",
    "dev:rn": "npm i && npm run dev",
    "dev": "react-native bundle-harmony --dev --minify=false",
    "dev:hbc": "react-native bundle-harmony --dev --bundle-output /tmp/bundle.harmony.js && hermesc --emit-binary /tmp/bundle.harmony.js -out ./harmony/entry/src/main/resources/rawfile/hermes_bundle.hbc",
    "dev:multi_surface": "react-native bundle-harmony --dev --bundle-output './harmony/multi_surface/src/main/resources/rawfile/bundle.harmony.js' --assets-dest './harmony/multi_surface/src/main/resources/rawfile/assets'",
    "android": "react-native run-android",
    "format": "prettier App.tsx ./benchmarks ./components ./examples ./jests ./scripts ./tests -w",
    "typecheck": "tsc --noEmit",
    "lint": "eslint App.tsx ./benchmarks ./components ./examples ./jests ./scripts ./tests",
    "lint:fix": "npm run lint -- --fix",
    "verify": "npm run typecheck && npm run test && npm run lint",
    "start": "react-native start",
    "benchmark-fps": "node ./scripts/get-frame-times | node ./scripts/create-fps-stats",
    "test": "jest"
  },
  "dependencies": {
    "@gorhom/portal": "^1.0.14",
    "@rnoh/testerino": "^0.0.8",
    "chai": "4.3.7",
    "d3-scale-chromatic": "^3.0.0",
    "react": "18.2.0",
    "react-native": "0.72.5",
    "react-native-flipper-performance-plugin": "^0.4.0",
    "react-native-harmony": "file:../react-native-harmony/rnoh-react-native-harmony-0.72.11.tgz"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native/eslint-config": "^0.74.0",
    "@react-native/metro-config": "^0.72.6",
    "@tsconfig/react-native": "^2.0.2",
    "@types/chai": "^4.3.4",
    "@types/d3-scale-chromatic": "^3.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.5",
    "@types/metro-config": "^0.76.2",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "csv-parser": "^3.0.0",
    "eslint": "^8.19.0",
    "eslint-plugin-prettier": "^5.0.1",
    "fs-extra": "^11.1.1",
    "jest": "^29.7.0",
    "json5": "^2.2.3",
    "metro": "^0.76.3",
    "metro-config": "^0.76.3",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^3.0.2",
    "react-test-renderer": "18.2.0",
    "simple-statistics": "^7.8.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2",
    "yargs": "^17.7.2"
  },
  "overrides": {
    "@react-native-community/cli": "11.3.6"
  },
  "resolutions": {
    "@react-native-community/cli": "11.3.6"
  }
}
